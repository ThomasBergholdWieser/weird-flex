{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "bootstrap@4.5.2",
      "destination": "wwwroot/lib/bootstrap",
      "files": [
        "dist/css/*",
        "dist/js/*"
      ]
    },
    {
      "provider": "unpkg",
      "library": "chart.js@2.7.1",
      "destination": "wwwroot/lib/chartjs",
      "files": [
        "dist/Chart.bundle.js",
        "dist/Chart.bundle.min.js",
        "dist/Chart.js",
        "dist/Chart.min.js"
      ]
    }
  ]
}